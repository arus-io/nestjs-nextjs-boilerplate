{
  "name": "nestjs-nextjs-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config server/entryPointOrm",
    "start:dev": "ts-node server/main",
    "start:dev:fe": "PORT=3000 ts-node server/_next.dev",
    "start:dev:be": "PORT=3001 NO_FE=1 nodemon --ext ts,js,json --watch ./server --exec \"ts-node server/main\"",
    "start:dev:be:fast": "ORT=3001 NO_FE=1 nest build --webpack --webpackPath webpack-hmr.config.js",
    "test": "jest --runInBand --detectOpenHandles --forceExit",
    "test:ci": "jest --silent --forceExit",
    "test:dev": "jest --runInBand --detectOpenHandles --watch",
    "test:regex": "jest --runInBand -t",
    "start": "NODE_ENV=production node dist/server/main",
    "build": "npm run build:next && npm run build:server && npm run copy:dist",
    "build:next": "next build client",
    "build:server": "nest build",
    "copy:dist": "cp -r static dist/static/",
    "lint": "eslint .",
    "postinstall": "if [ \"$NODE_ENV\" = production ]; then npm run typeorm migration:run; fi;",
    "storybook": "start-storybook",
    "codegen:gql": "graphql-codegen --config codegen.yml",
    "run-task": "ts-node server/entryPointTasks.ts"
  },
