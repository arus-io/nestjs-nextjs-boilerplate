  "jest": {
    "preset": "ts-jest",
    "verbose": false,
    "globalSetup": "<rootDir>/server/test-utils/_jest.setup.ts",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](.next|node_modules|scripts|dist)[/\\\\]"
    ],
    "collectCoverageFrom": [
      "<rootDir>/server/**/*.(t|j)s"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "coverageDirectory": "./coverage"
  },
